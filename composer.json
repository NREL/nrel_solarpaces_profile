{
  "name": "nrel/nrel_solarpaces_profile",
  "description": "Custom profile for the NREL SolarPaces project. Built by Fruition.net.",
  "type": "drupal-profile",
  "homepage": "https://fruition.net",
  "authors": [
    {
      "name": "Drew Michael",
      "email": "drew@fruition.net",
      "role": "Maintainer"
    }
  ],
  "support": {
      "issues": "https://github.com/NREL/nrel_solarpaces_profile/issues"
  },
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
      "drupal/stage_file_proxy": "^1.0@alpha"
  },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.test"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "preferred-install": {
            "drupal/core": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.1.0"
        },
        "secure-http": false
    },
  "extra": {
      "installer-types": [
          "bower-asset",
          "npm-asset"
      ],
      "installer-paths": {
          "docroot/core": [
              "type:drupal-core"
          ],
          "docroot/libraries/{$name}": [
              "type:drupal-library",
              "type:bower-asset",
              "type:npm-asset"
          ],
          "docroot/modules/contrib/{$name}": [
              "type:drupal-module"
          ],
          "docroot/profiles/contrib/{$name}": [
              "type:drupal-profile"
          ],
          "docroot/themes/contrib/{$name}": [
              "type:drupal-theme"
          ]
      }
  },
  "patches": {
  },
  "require": {
    "drupal/conditional_fields": "^1.0@alpha",
    "drupal/config_update": "^1.5",
    "drupal/default_content": "^1.0@alpha",
    "drupal/field_collection": "^1.0@alpha",
    "drupal/lightning_workflow": "^1.0@alpha",
    "drupal/node_clone": "1.x-dev",
    "drupal/paragraphs": "^1.1",
    "drupal/radix_layouts": "^4.0",
    "drupal/redirect": "^1.0@beta",
    "drupal/scheduled_updates": "^1.0@alpha",
    "nrel/communications": "~3.1.0"
  }
}
