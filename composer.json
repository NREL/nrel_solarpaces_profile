{
  "name": "nrel/nrel_solarpaces_profile",
  "description": "Custom profile for the NREL SolarPaces project. Built by Fruition.net.",
  "type": "drupal-profile",
  "homepage": "https://fruition.net",
  "authors": [
    {
      "name": "Drew Michael",
      "email": "drew@fruition.net",
      "role": "Maintainer"
    }
  ],
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
      "drupal/stage_file_proxy": "^1.0@alpha"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
        "type": "composer",
        "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "bin-dir": "bin/"
  },
  "extra": {
      "installer-types": [
          "bower-asset",
          "npm-asset"
      ],
      "packagist.drupal-composer.org": {
          "discover-make-files": false
      },
      "installer-paths": {
          "docroot/core": [
              "type:drupal-core"
          ],
          "docroot/libraries/{$name}": [
              "type:drupal-library",
              "type:bower-asset",
              "type:npm-asset"
          ],
          "docroot/modules/contrib/{$name}": [
              "type:drupal-module"
          ],
          "docroot/profiles/contrib/{$name}": [
              "type:drupal-profile"
          ],
          "docroot/themes/contrib/{$name}": [
              "type:drupal-theme"
          ]
      }
  },
  "require": {
    "nrel/communications": "^1.1.1.18",
    "drupal/radix_layouts": "^4.0",
    "drupal/field_collection": "^1.0@alpha",
    "drupal/node_clone": "1.x-dev",
    "drupal/default_content": "^1.0@alpha",
    "drupal/redirect": "^1.0@beta",
    "drupal/paragraphs": "^1.1",
    "drupal/conditional_fields": "^1.0@alpha"
  }
}
